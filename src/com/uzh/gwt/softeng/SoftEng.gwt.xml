<?xml version="1.0" encoding="UTF-8"?>

<module rename-to="SoftEng">
	<!--  As always, we need to inherit the User module -->
	<inherits name="com.google.gwt.user.User"/>
	 <inherits name="com.googlecode.gwt.charts.Charts"/>
	 <inherits name="org.spiffyui.spiffyui" />

	<!-- We tell the GWT compiler to compile all code in and below the client package -->
	<source path="client" />
	<source path="server" />
	<source path='shared'/>
	
	<!--  We choose to use the built in CSS Chrome theme -->
    <inherits name='com.google.gwt.user.theme.chrome.Chrome'/>
    <!-- <inherits name='com.google.gwt.user.theme.clean.Clean'/>   -->
    <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>  -->
    <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/>  -->
    
    <!-- Here we define the class that contains the entry point -->
	<entry-point class="com.uzh.gwt.softeng.client.ApplicationLogic"/>
	
	
</module>
